#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xd9b730 .scope module, "inp_tb" "inp_tb" 2 1;
 .timescale 0 0;
v0xde1da0_0 .var "but_inp", 0 0;
v0xde1e70_0 .var "input_val", 7 0;
v0xde1f20_0 .net "instrMem", 63 0, v0xde1d00_0; 1 drivers
S_0xd9b820 .scope module, "testing" "inp" 2 7, 3 1, S_0xd9b730;
 .timescale 0 0;
v0xdd0cf0_0 .net "but_inp", 0 0, v0xde1da0_0; 1 drivers
v0xde1c60_0 .net "input_val", 7 0, v0xde1e70_0; 1 drivers
v0xde1d00_0 .var "instrMem", 63 0;
E_0xdacd00 .event edge, v0xdd0cf0_0;
    .scope S_0xd9b820;
T_0 ;
    %wait E_0xdacd00;
    %load/v 8, v0xde1c60_0, 8;
    %load/v 16, v0xde1d00_0, 64;
    %set/v v0xde1d00_0, 8, 64;
    %vpi_call 3 8 "$display", "Instruction bits till now are : %b", v0xde1d00_0;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0xd9b730;
T_1 ;
    %set/v v0xde1da0_0, 1, 1;
    %movi 8, 136, 8;
    %set/v v0xde1e70_0, 8, 8;
    %set/v v0xde1da0_0, 0, 1;
    %delay 1000, 0;
    %movi 8, 137, 8;
    %set/v v0xde1e70_0, 8, 8;
    %set/v v0xde1da0_0, 1, 1;
    %delay 1000, 0;
    %movi 8, 138, 8;
    %set/v v0xde1e70_0, 8, 8;
    %set/v v0xde1da0_0, 0, 1;
    %delay 1000, 0;
    %movi 8, 140, 8;
    %set/v v0xde1e70_0, 8, 8;
    %set/v v0xde1da0_0, 1, 1;
    %delay 1000, 0;
    %movi 8, 144, 8;
    %set/v v0xde1e70_0, 8, 8;
    %set/v v0xde1da0_0, 0, 1;
    %delay 1000, 0;
    %movi 8, 168, 8;
    %set/v v0xde1e70_0, 8, 8;
    %set/v v0xde1da0_0, 1, 1;
    %delay 1000, 0;
    %movi 8, 137, 8;
    %set/v v0xde1e70_0, 8, 8;
    %set/v v0xde1da0_0, 0, 1;
    %delay 1000, 0;
    %set/v v0xde1e70_0, 1, 8;
    %set/v v0xde1da0_0, 1, 1;
    %vpi_call 2 34 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "input_mode_tb.v";
    "input_mode.v";
